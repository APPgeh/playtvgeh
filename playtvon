<script>
    // Função para atualizar os iframes a cada 15 segundos
    function updateIframes() {
      document.getElementById("iframe1").src += "&random=" + Math.random(); // Adiciona um parâmetro aleatório à URL para forçar o recarregamento
      document.getElementById("iframe2").src += "&random=" + Math.random(); // Adiciona um parâmetro aleatório à URL para forçar o recarregamento
      document.getElementById("iframe3").src += "&random=" + Math.random(); // Adiciona um parâmetro aleatório à URL para forçar o recarregamento
    }

    // Chama a função a cada 15 segundos
    setInterval(updateIframes, 15000);
  </script>
